{
  "name": "@contentful/apps",
  "private": true,
  "engines": {
    "node": ">=16.0.0 < 19.0.0",
    "npm": ">= 8.0.0 < 11.0.0"
  },
  "packageManager": "pnpm@v9.1.2",
  "devDependencies": {
    "@sentry/cli": "^2.14.4",
    "eslint": "^7.32.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.0",
    "prettier": "^2.8.8"
  },
  "scripts": {
    "clean": "lerna clean",
    "post-deploy": "lerna run post-deploy",
    "publish-packages": "lerna version --conventional-commits --no-private --force-git-tag --create-release github --yes && lerna publish from-git --yes",
    "prettier:list": "npx prettier@2.8.8 --list-different",
    "prettier:write": "npx prettier@2.8.8 --write",
    "prettier:check": "npx prettier@2.8.8 --check '**/*.{js,jsx,ts,tsx}'",
    "prepare": "husky install",
    "sentry-release": "SENTRY_RELEASE=$(git rev-parse --short HEAD); sentry-cli releases --log-level=debug new -p marketplace-apps $SENTRY_RELEASE && sentry-cli releases --log-level=debug set-commits --auto --ignore-missing $SENTRY_RELEASE && sentry-cli releases --log-level=debug finalize $SENTRY_RELEASE && sentry-cli releases --log-level=debug deploys $SENTRY_RELEASE new -e production"
  },
  "dependencies": {
    "lerna": "^8.1.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run prettier:write"
    ]
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "pnpm": {
    "overrides": {
      "node-notifier@<8.0.1": ">=8.0.1",
      "node-forge@<1.0.0": ">=1.0.0",
      "node-forge@<1.3.0": ">=1.3.0",
      "got@<11.8.5": ">=11.8.5",
      "jsdom@<=16.4.0": ">=16.5.0",
      "ejs@<3.1.7": ">=3.1.7",
      "jsonwebtoken@<=8.5.1": ">=9.0.0",
      "graphiql@>=0.5.0 <1.4.7": ">=1.4.7",
      "node-fetch@<2.6.1": ">=2.6.1",
      "jsonwebtoken@<9.0.0": ">=9.0.0",
      "markdown-it@<12.3.2": ">=12.3.2",
      "browserslist@>=4.0.0 <4.16.5": ">=4.16.5",
      "loader-utils@>=2.0.0 <2.0.3": ">=2.0.3",
      "postcss@<8.4.31": ">=8.4.31",
      "ansi-html@<0.0.8": ">=0.0.8",
      "glob-parent@<5.1.2": ">=5.1.2",
      "loader-utils@>=2.0.0 <2.0.4": ">=2.0.4",
      "node-fetch@<2.6.7": ">=2.6.7",
      "nth-check@<2.0.1": ">=2.0.1",
      "shell-quote@<=1.7.2": ">=1.7.3",
      "luxon@>=1.0.0 <1.28.1": ">=1.28.1",
      "qs@>=6.7.0 <6.7.3": ">=6.7.3",
      "semver@>=7.0.0 <7.5.2": ">=7.5.2",
      "minimatch@<3.0.5": ">=3.0.5",
      "axios@>=0.8.1 <0.28.0": ">=0.28.0",
      "tough-cookie@<4.1.3": ">=4.1.3",
      "webpack-dev-middleware@<=5.3.3": ">=5.3.4",
      "express@<4.19.2": ">=4.19.2",
      "protobufjs@>=7.0.0 <7.2.5": ">=7.2.5",
      "immer@>=7.0.0 <9.0.6": ">=9.0.6",
      "ejs@<3.1.10": ">=3.1.10",
      "braces@<3.0.3": ">=3.0.3",
      "ws@>=8.0.0 <8.17.1": ">=8.17.1",
      "@azure/identity@<4.2.1": ">=4.2.1"
    }
  }
}
